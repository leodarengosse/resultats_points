function Dist = dist_cart(A,B,i)

    if ~exist('i','var')
        i
        Dist = sqrt((A.Xi-B.X)^2+(A.Yi-B.Y)^2+(A.Zi-B.Z)^2);
    else
        Dist = sqrt((A.X-B.X)^2+(A.Y-B.Y)^2+(A.Z-B.Z)^2);
        
end
if ~exist('param3','var')
     % third parameter does not exist, so default it to something
      param3 = 42;
 end